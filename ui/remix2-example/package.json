{
  "name": "remix2-example",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "build": "remix build",
    "dev": "OTEL_SERVICE_NAME=spike-ui-api-otel-remix2 node --require @opentelemetry/auto-instrumentations-node/register ./node_modules/.bin/remix dev --manual",
    "start": "OTEL_SERVICE_NAME=spike-ui-api-otel-remix2 node --require @opentelemetry/auto-instrumentations-node/register ./node_modules/.bin/remix-serve ./build/index.js",
    "typecheck": "tsc"
  },
  "dependencies": {
    "@opentelemetry/api": "^1.6.0",
    "@opentelemetry/api-logs": "^0.43.0",
    "@opentelemetry/auto-instrumentations-node": "^0.39.2",
    "@remix-run/css-bundle": "^2.0.0",
    "@remix-run/node": "^2.0.0",
    "@remix-run/react": "^2.0.0",
    "@remix-run/serve": "^2.0.0",
    "isbot": "^3.6.8",
    "kit": "file:../kit",
    "oauth4webapi": "^2.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@remix-run/dev": "^2.0.0",
    "@remix-run/eslint-config": "^2.0.0",
    "@types/react": "^18.2.20",
    "@types/react-dom": "^18.2.7",
    "eslint": "^8.38.0",
    "typescript": "^5.1.6"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
